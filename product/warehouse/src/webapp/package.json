{
  "name": "codekvast-warehouse",
  "version": "0.18.5",
  "description": "The web interface for Codekvast Warehouse",
  "private": true,
  "scripts": {
    "build": "webpack --config config/webpack.prod.js",
    "build:q": "webpack --config config/webpack.dev.js --progress",
    "clean": "rimraf dist",
    "lint": "tslint ./src/**/*.ts -t verbose",
    "postinstall": "typings install",
    "prebuild": "rimraf dist",
    "start": "webpack-dev-server --inline --progress --port 8088",
    "test": "karma start && npm run ts-coverage-remap",
    "test:w": "karma start --no-single-run --auto-watch",
    "ts-coverage-remap": "remap-istanbul -i ../../build/reports/frontend-coverage/coverage-final.json -o ../../build/reports/frontend-coverage -t html"
  },
  "license": "MIT",
  "dependencies": {
    "@angular/common": "4.0.1",
    "@angular/compiler": "4.0.1",
    "@angular/core": "4.0.1",
    "@angular/forms": "4.0.1",
    "@angular/http": "4.0.1",
    "@angular/platform-browser": "4.0.1",
    "@angular/platform-browser-dynamic": "4.0.1",
    "@angular/router": "4.0.1",
    "@ng-bootstrap/ng-bootstrap": "1.0.0-alpha.22",
    "core-js": "2.4.1",
    "intl": "1.2.5",
    "rxjs": "5.2.1-smooth",
    "zone.js": "0.8.5"
  },
  "devDependencies": {
    "angular2-template-loader": "0.6.2",
    "awesome-typescript-loader": "3.1.2",
    "css-loader": "0.28.0",
    "extract-text-webpack-plugin": "2.1.0",
    "file-loader": "0.11.1",
    "html-loader": "0.4.5",
    "html-webpack-plugin": "2.28.0",
    "html-webpack-template": "6.0.1",
    "istanbul-instrumenter-loader": "2.0.0",
    "jasmine-core": "2.5.2",
    "karma": "1.5.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-coverage": "1.1.1",
    "karma-firefox-launcher": "1.0.1",
    "karma-jasmine": "1.1.0",
    "karma-junit-reporter": "1.2.0",
    "karma-mocha-reporter": "2.2.3",
    "karma-phantomjs-launcher": "1.0.4",
    "karma-source-map-support": "1.2.0",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "2.0.3",
    "null-loader": "0.1.1",
    "raw-loader": "0.5.1",
    "reflect-metadata": "0.1.10",
    "remap-istanbul": "0.9.5",
    "rimraf": "2.6.1",
    "source-map-loader": "0.2.1",
    "style-loader": "0.16.1",
    "ts-loader": "2.0.3",
    "tslint": "5.0.0",
    "typescript": "2.2.2",
    "typings": "2.1.0",
    "webpack": "2.3.2",
    "webpack-dev-server": "2.4.2",
    "webpack-merge": "4.1.0"
  }
}
